<p id="notice"><%= notice %></p>

<h1>Cashmeouts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      <th>Drop</th>
      <th>Sub total</th>
      <th>Total delivery</th>
      <th>Del charge</th>
      <th>Extra</th>
      <th>Toget</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cashmeouts.each do |cashmeout| %>
      <tr>
        <td><%= cashmeout.date %></td>
        <td><%= cashmeout.name %></td>
        <td><%= cashmeout.drop %></td>
        <td><%= cashmeout.sub_total %></td>
        <td><%= cashmeout.total_delivery %></td>
        <td><%= cashmeout.del_charge %></td>
        <td><%= cashmeout.extra %></td>
        <td><%= cashmeout.toget %></td>
        <td><%= link_to 'Show', cashmeout %></td>
        <td><%= link_to 'Edit', edit_cashmeout_path(cashmeout) %></td>
        <td><%= link_to 'Destroy', cashmeout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cashmeout', new_cashmeout_path %>
